{
  "extends": [
    "tslint-config-standard",
    "tslint-react"
  ],
  "rules": {
    "prettier": true,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "jsx-no-multiline-js": false,
    "semicolon": [true, "always"],
    "quotemark": [true, "single"],
    "object-curly-spacing": [true, "never"],
    "space-before-function-paren": [true, "never"]
  }
}
