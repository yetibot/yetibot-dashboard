{
  "name": "yetibot-dashboard",
  "version": "0.5.3",
  "description": "Dashboard that runs against Yetibot GraphQL API",
  "keywords": [
    "babel",
    "babel 7",
    "typescript",
    "react",
    "yetibot",
    "graphql"
  ],
  "main": "./dist/index.js",
  "repository": "https://github.com/yetibot/yetibot-dashboard",
  "author": "Trevor C. Hartman",
  "license": "EPL-1.0",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "clean": "rimraf dist .cache-loader",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "prepublishOnly": "yarn build",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test -o --watch",
    "lint": "tslint \"src/**/*.ts\""
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "typescript-babel-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-3": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "@types/enzyme": "^3.1.9",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^22.1.3",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.25",
    "apollo-codegen": "^0.19.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "cache-loader": "^1.2.0",
    "cross-env": "^5.1.3",
    "cross-fetch": "^2.2.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "npm:enzyme-react-adapter-future",
    "file-loader": "^1.1.11",
    "fork-ts-checker-notifier-webpack-plugin": "^0.2.0",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.15.1",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "svg-react-loader": "^0.4.5",
    "thread-loader": "^1.1.2",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "typescript-babel-jest": "^1.0.5",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "unfetch": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-fail-plugin": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@types/query-string": "^6.1.0",
    "apollo-boost": "^0.1.6",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-link-http": "^1.5.4",
    "bloomer": "^0.6.3",
    "bulma": "^0.7.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "moment": "^2.22.1",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions"
  ]
}
