      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "typescript-babel-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
